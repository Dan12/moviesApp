<div class="container">
  <div class="row">
    <div class="col-md-12">
      <h1>Movies</h1>
    </div>
  </div>
  <div class="row">
    <% @movies.each do |movie|%>
      <div class="col-md-3 col-sm-4 col-xs-6">
        <a href="/movie/<%=movie.id%>" class="thumbnail">
          <img src="<%=movie.imgurl%>" alt="<%=movie.name%>">
        </a>
      </div>
    <% end %>
  </div>
  <a href = "/newmovie">New Movie</a>
</div>